{
  "manifest_version": 2,

  "name": "Daily Tracker",
  "description": "This extension allows the user to Add daily task.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "http://www.google.com/",
    "https://www.google.com/",
    "https://script.google.com/"
  ]
}